{
  "name": "cortiware",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "setpwd": "tsx scripts/set-password.ts",
    "setup-accounts": "tsx scripts/setup-accounts.ts",
    "dev": "turbo run dev --parallel",
    "dev:root": "next dev -p 5000",
    "build": "turbo run build",
    "build:root": "next build",
    "vercel-build": "node node_modules/prisma/build/index.js generate && node node_modules/next/dist/bin/next build",
    "start": "next start",
    "seed": "tsx prisma/seed.ts",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:unit": "tsx tests/unit/run.ts",
    "test:e2e": "tsx tests/e2e/run.ts",
    "contracts:gen": "node scripts/generate-contract-snapshot.js",
    "contracts:diff": "node scripts/diff-contracts.js --fail-on-breaking",
    "contracts:check": "npm run contracts:gen && node scripts/diff-contracts.js --fail-on-breaking",
    "contracts:promote": "node scripts/promote-contracts.js"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@sendgrid/mail": "8.1.0",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "@tremor/react": "^3.18.7",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "daisyui": "^5.1.27",
    "flowbite": "^3.1.2",
    "flowbite-react": "^0.12.9",
    "ioredis": "^5.8.1",
    "lucide-react": "^0.545.0",
    "next": "^15.1.0",
    "openai": "^5.23.0",
    "otplib": "^12.0.1",
    "pdfkit": "^0.17.2",
    "preline": "^3.2.3",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^3.2.1",
    "stripe": "^14.25.0",
    "swr": "^2.2.5",
    "tailwind-merge": "^3.3.1",
    "twilio": "4.22.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.18.6",
    "@types/pdfkit": "^0.17.3",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "18.2.22",
    "@types/stripe": "^8.0.416",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "autoprefixer": "10.4.20",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "postcss": "8.4.47",
    "prisma": "^6.16.2",
    "tailwindcss": "3.4.10",
    "tsx": "^4.20.5",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2"
  }
}
