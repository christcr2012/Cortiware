# Local development environment variables
# Copy this to .env.local and fill in your values

# Database (required for build)
DATABASE_URL="postgresql://user:password@localhost:5432/streamflow?schema=public"

# Federation (optional)
FED_ENABLED=false
FED_OIDC_ENABLED=false

# Provider/Developer/Accountant credentials (optional)
# PROVIDER_TOKEN=dev-provider
# DEVELOPER_TOKEN=dev-developer
# ACCOUNTANT_TOKEN=dev-accountant

