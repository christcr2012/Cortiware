{
  "buildCommand": "cd ../.. && npm run build -- --filter=marketing-robinson",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "cd ../.. && npm install",
  "ignoreCommand": "npx turbo-ignore marketing-robinson",
  "rewrites": [
    {
      "source": "/portal/:path*",
      "destination": "https://provider-portal.vercel.app/:path*"
    }
  ],
  "github": {
    "silent": true,
    "autoJobCancelation": false
  }
}

