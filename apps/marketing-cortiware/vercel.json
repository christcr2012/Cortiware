{
  "buildCommand": "cd ../.. && npm run build -- --filter=marketing-cortiware",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "cd ../.. && npm install",
  "ignoreCommand": "npx turbo-ignore marketing-cortiware",
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "https://tenant-app.vercel.app/:path*"
    }
  ],
  "github": {
    "silent": true,
    "autoJobCancelation": false
  }
}

